#!/usr/bin/env node

'use strict';

var program = require('commander');

program
  .version('0.0.0')
  .command('sshKeys', 'Manage ssh keys for provisioning machines')
  .command('providerConfigs', 'Manager provider-specific configurations')
  .command('machineConfigOptions', 'View config parameters for machines')
  .command('provisionedMachines', 'Provision machines')
  .parse(process.argv);
